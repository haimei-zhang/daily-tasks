/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~service/*": [
        "src/app/service/*"
      ],
      "~components/*": [
        "src/app/components/*"
      ],
      "~view/*": [
        "src/app/view/*"
      ],
      "~directives/*": [
        "src/app/directives/*"
      ],
      "~views/*": [
        "src/app/views/*"
      ],
      "~models/*": [
        "src/app/models/*"
      ],
      "~utils/*": [
        "src/app/utils/*"
      ],
      "~libs/*": [
        "src/app/libs/*"
      ],
      "~pipes/*": [
        "src/app/pipes/*"
      ],
      "~constants": [
        "constants"
      ]
    },
    "outDir": "./docs/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
